{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".tmp",
    "test/"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "templates/",
    "assets/",
    "config/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json jade"
}
